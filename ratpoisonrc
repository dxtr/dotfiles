chdir
addhook switchwin banish
startup_message off
set bargravity c
set border 0
set barborder 1
set font Terminus:pixelsize=12
set bgcolor #000000
set fgcolor #00FF00

exec /usr/bin/rpws init 5 -k

bind h rpwsp
bind l rpwsn
bind j focusprev
bind k focus
bind d exec xxxterm
bind F1 exec /usr/bin/rpws 1
bind F2 exec /usr/bin/rpws 2
bind F3 exec /usr/bin/rpws 3
bind F4 exec /usr/bin/rpws 4
bind F5 exec /usr/bin/rpws 5

